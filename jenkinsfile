pipeline {
  agent any
    
  tools {nodejs "node"}
    
  stages {
        
    stage('Git') {
      steps {
        git 'https://github.com/devopstwilightsoftwares/Demoapplication.git'
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
         sh 'npm run start'
      }
    }  
    
            
    stage('Test') {
      steps {
        sh 'node test'
      }
    }
  }
}